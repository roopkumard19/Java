public class Application {

	public static void main(String[] args) {
		int myNumber = 112;
		short myShort = 23; //shorter version of int
		long myLong = 234444; //longer version of int
		
		double myDouble = 23.4534;
		float myFloat = 234.34f; //shorter version of double.Pls note the "f" at the end of the value declared.
		
		System.out.println(myNumber);
		System.out.println(myShort);
		System.out.println(myLong);
		System.out.println(myDouble);
		System.out.println(myFloat);
	}

}
